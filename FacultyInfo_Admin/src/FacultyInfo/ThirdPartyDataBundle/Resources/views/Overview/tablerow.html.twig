<tr>
	<td>{{ nameTrans|trans }}</td>
	<td>{{ lastUpdate|date('d.m.Y H:i') }}</td>
	<td>
		{% if lastStatuscode == 0 %}
			<img src="{{ asset('img/buttons/check.png') }}" class="success" />
		{% else %}
			<img src="{{ asset('img/buttons/error.png') }}" class="error" />
		{% endif %}		
	</td>
	<td>
		{{ include(':Button:edit.html.twig', { 'url' : url('facultyinfo_thirdparty_update', { 'id' : name } ) } ) }}
		<a id="{{ name }}ReloadButton" class="reloadButton" href="javascript:void(0)" onclick="loadData('http://lvps176-28-10-221.dedicated.hosteurope.de/load/{{ name }}', '{{ name }}')" title="{% trans %}button.refresh{% endtrans %}"><img class="tableButton" src="{{ asset('img/buttons/reload.png') }}" /></a>
		<img style="display:none" id="{{ name }}LoadingIndicator" class="tableIcon" src="{{ asset('img/buttons/loading.gif') }}" />
	</td>
</tr>